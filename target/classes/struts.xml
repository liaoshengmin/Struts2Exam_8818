<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="test" namespace="" extends="struts-default">

		<action name="login" class="com.action.LoginAction" method="execute">
			<result name="success" type="redirectAction">show</result>
			<result name="error">/login.jsp</result>
			<result name="input">/login.jsp</result>
		</action>
		
		<action name="show" class="com.action.ShowAction" method="execute">
			<result name="success">/index.jsp</result>
		</action>
		
		<action name="getAddress" class="com.action.GetAddressAction" method="execute">
			<result name="success">/add.jsp</result>
		</action>
		
		<action name="add" class="com.action.AddAction" method="execute">
			<result name="success" type="redirectAction">show</result>
		</action>
		
		<action name="delete" class="com.action.DeleteAction" method="execute">
			<result name="success" type="redirectAction">show</result>
		</action>
		


	</package>
</struts>

